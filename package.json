  "dependencies": {
    "bcrypt": "^5.0.0",
    "chance": "^1.1.7",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.18",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.6",
    "redux-devtools-extension": "^2.13.8"
  }
